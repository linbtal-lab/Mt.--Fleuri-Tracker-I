  "rag_system": {
    "description": "RAG confidence ratings based on current grade vs target grade",
    "green": {
      "label": "Green",
      "meaning": "At or above target grade",
      "condition": "current_grade_numeric >= target_grade_numeric",
      "action": "Continue current approach; stretch and challenge"
    },
    "amber": {
      "label": "Amber",
      "meaning": "Within 1 full grade below target",
      "condition": "current_grade_numeric >= (target_grade_numeric - 1)",
      "action": "Monitor closely; light-touch intervention recommended"
    },
    "red": {
      "label": "Red",
      "meaning": "More than 1 full grade below target",
      "condition": "current_grade_numeric < (target_grade_numeric - 1)",
      "action": "Immediate intervention required; contact parents"
    }
  },

